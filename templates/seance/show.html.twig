{% extends 'base.html.twig' %}

{% block title %}Seance
{% endblock %}

{% block body %}
	<h1>Seance</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ seance.id }}</td>
			</tr>
			<tr>
				<th>Username</th>
				<td>{{ seance.user.username }}</td>
			</tr>
			<tr>
				<th>Duration</th>
				<td>{{ seance.duration }}</td>
			</tr>
			<tr>
				<th>Percieved_intensity</th>
				<td>{{ seance.percievedIntensity }}</td>
			</tr>
			<tr>
				<th>Burned_calories</th>
				<td>{{ seance.burnedCalories }}</td>
			</tr>
			<tr>
				<th>Body_temperature_delta</th>
				<td>{{ seance.bodyTemperatureDelta }}</td>
			</tr>
			<tr>
				<th>Muscle_fatigue</th>
				<td>{{ seance.muscleFatigue }}</td>
			</tr>
			<tr>
				<th>Heart_recovery_rate</th>
				<td>{{ seance.heartRecoveryRate }}</td>
			</tr>
			<tr>
				<th>Heartbeat_variation</th>
				<td>{{ seance.heartbeatVariation }}</td>
			</tr>
			<tr>
				<th>Date</th>
				<td>{{ seance.date ? seance.date|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_seance_index') }}">back to list</a>

	<a href="{{ path('app_seance_edit', {'id': seance.id}) }}">edit</a>

	{{ include('seance/_delete_form.html.twig') }}
{% endblock %}
